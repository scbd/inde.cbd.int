<!DOCTYPE html>
<html lang="en" ng-controller="TemplateController">

<head>
  <base href="<%=baseUrl%>">
  <title>Side Events - Convention on Biological Diversity</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link rel="shortcut icon" href="app/images/favicon.ico">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link href="app/libs/font-awesome/css/font-awesome.min.css" rel="stylesheet">
  <link href="app/libs/angular-toastr/dist/angular-toastr.min.css" rel="stylesheet">
  <link href="app/libs/flag-icon-css/css/flag-icon.css" rel="stylesheet">
  <link href="app/libs/ng-dialog/css/ngDialog.css" rel="stylesheet">
  <link href="app/libs/ng-dialog/css/ngDialog-theme-default.min.css" rel="stylesheet">
  <link href="app/css/main.css" rel="stylesheet">
  <style>
    [ng\:cloak],
    [ng-cloak],
    .ng-cloak {
      display: none !important;
    }

    body,
    .greybg {
      background: #f4f4f4;
    }

    .whitebg {
      background: #FFFFFF;
    }

    header.mobile.navbar {
      color: #009B48;
      background-color: white;
    }

    header.mobile.navbar a {
      color: #009B48;
    }

    header.mobile .navbar-nav a:hover {
      color: white;
      background-color: #009B48
    }

    header.mobile .navbar-header {
      font-weight: semi-bold;
    }

    header.mobile .navbar-header img.logo {
      height: 55px;
      vertical-align: middle;
      margin-left: 3px;
    }

    header.large> div.container> div {
      border-bottom: solid 1px #f4f4f4;
    }

    header.large img.logo {
      height: 70px
    }

    header.large .navbar-nav a {
      color: #009B48;
    }

    header.large .navbar-nav a:hover {
      color: white;
      background-color: #009B48;
    }

    footer {
      display: block;
      padding: 15px 20px;
    }

    footer ul {
      list-style-type: none;
      padding: 0px;
    }

    footer ul li a {
      font-size: 14px;
      color: #666;
    }

    footer ul li a:hover {
      color: #000;
    }

    footer h4 {
      font-size: 14px;
      color: #666;
      font-weight: bold;
    }

    footer.standard ul.list-inline> li {
      padding: 0px 5px;
      border-right: solid 1px #666;
      font-size: 14px;
    }

    footer.standard ul.list-inline> li:last-child {
      border-right: none;
    }

    h1 {
      font-size: 28px;
    }

    .view h1,
    .view h2,
    .view h3,
    .view h4,
    .view h5,
    .view h6,
    .view .h1,
    .view .h2,
    .view .h3,
    .view .h4,
    .view .h5,
    .view .h6 {
      font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
      color: #009B48;
    }

    @media (min-width: 768px) {
      button.hidden-xs {
        display: inline-block !important;
      }
      span.hidden-xs {
        display: inline !important;
      }
    }

    @media (max-width: 767px) {
      button.visible-xs {
        display: inline-block !important;
      }
      span.visible-xs {
        display: inline !important;
      }
    }

    .growl {
      width: 50%;
      position: fixed;
      right: 5px;
      margin: 0 0 0 -25%;
      top: 5px;
      z-index: 9999;
    }

    .toast-info {
      background-color: rgba(34, 34, 34, .9);
      padding: 15px 5px 15px 5px;
      font-weight: lighter;
      font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
      letter-spacing: 1.5px;
    }

    .toast-success {
      background-color: rgba(34, 34, 34, .9);
      padding: 15px 5px 15px 5px;
      font-weight: lighter;
      font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
      letter-spacing: 1.8px;
      font-size: 16px;
    }

    #toast-container>:hover {
      -moz-box-shadow: 0 0 9pt #000;
      -webkit-box-shadow: 0 0 9pt #000;
      box-shadow: 0 0 9pt #000;
      opacity: 1;
      cursor: pointer;
    }

    #toast-container> div .title {
      padding: 15px 15px 15px 15px;
    }
  </style>
</head>

<body class="ltr" id="top">
  <!-- HEADER (MOBILE) -->
  <header class="mobile navbar visible-xs " role="banner">
    <div class="container">

      <div class="navbar-header">
        <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target=".navbar-collapse">
          <i class="fa fa-bars" style="font-size:1.66em;"></i>
        </button>

        <div ng-cloak ng-show="user" class="pull-right">

          <a ng-hide="user.isAuthenticated" class="navbar-toggle collapsed" style="text-decoration:none;color:#009B48" href="{{ACCOUNTS_URI}}signin?returnUrl={{encodedReturnUrl()}}" style="color: black">
            <i class="fa fa-power-off" style="font-size:1.66em;"></i>
          </a>

          <div ng-show="user.isAuthenticated" class="dropdown">

            <button class="navbar-toggle collapsed" title="{{user.name}}" style="text-decoration:none;color:#009B48" type="button" id="dropdownMenuL" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
              <i class="fa fa-user" style="font-size:1.66em;"></i>
            </button>

            <ul class="dropdown-menu" aria-labelledby="dropdownMenuL">
              <li><a href="{{ACCOUNTS_URI}}/profile?returnurl={{encodedReturnUrl()}}"><i class="fa fa-user" style="font-size:15px"></i>&nbsp; Profile</a></li>
              <li><a href="{{ACCOUNTS_URI}}/password?returnurl={{encodedReturnUrl()}}"><i class="fa fa-lock" style="font-size:15px"></i>&nbsp; Password</a></li>
              <li><a href="/user/signout"><i class="fa fa-sign-out" style="font-size:15px"></i>&nbsp; Sign-out</a></li>
            </ul>
          </div>

        </div>
        <!--  end sign in-->

        <!-- <a href="/" target="_self">
          <a href="/">
            <img class="logo" src="/app/images/cbd-logo-green-en.svg" />
          </a>
        </a> -->
        <div id="t-headerLogo">
                    <a href="/">
                        <img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/logo-cbd-leaf-line.gif" id="CCbdLogoLeaf" height="65" width="52" alt="Convention on Biological Diversity">
                        <img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/logo-cbd-text-en.gif" id="CCbdLogoText" height="65" alt="Convention on Biological Diversity">
                    </a>
                </div>
      </div>

      <nav class="collapse navbar-collapse" role="navigation">
        <ul class="nav navbar-nav">
          <li class="active"><a href="/convention">The Convention</a></li>
          <li class="active"><a href="/biosafety">Cartagena Protocol</a></li>
          <li class="active"><a href="/abs">Nagoya Protocol</a></li>
          <li class="active"><a href="/programmes">Programmes</a></li>
          <li class="active"><a href="/information">Information</a></li>
          <li class="active"><a href="/secretariat">Secretariat</a></li>
        </ul>
      </nav>
    </div>
  </header>

  <!-- HEADER (LARGE) -->
  <header class="large hidden-xs  ">
    <div class="container whitebg">
      <div>

        <div ng-cloak ng-show="user" class="navbar-right" style="margin:5px">

          <a class="btn btn-link" style="text-decoration:none;color:#009B48" ng-hide="user.isAuthenticated" href="#" ng-click="auth.goToSignIn()" style="color: black">
            <i class="fa fa-power-off"></i> Sign in
          </a>

          <div ng-show="user.isAuthenticated" class="dropdown">
            <button class="btn btn-link dropdown-toggle" style="text-decoration:none;color:#009B48" type="button" id="dropdownMenuL" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
              <span class="caret"></span> {{user.name}}
            </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenuL">
              <li ng-click="auth.editProfile()"><a href="#"><i class="fa fa-user" style="font-size:15px"></i>&nbsp; Profile</a></li>
              <li ng-click="auth.passwordReset()"><a href="#"><i class="fa fa-lock" style="font-size:15px"></i>&nbsp; Password</a></li>
              <li ng-click="auth.goToSignOut()"><a href="#"><i class="fa fa-sign-out" style="font-size:15px"></i>&nbsp; Sign-out</a></li>
            </ul>
          </div>

        </div>
        <!--  end sign in-->
        <div id="t-headerLogo">
                    <a href="/">
                        <img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/logo-cbd-leaf-line.gif" id="CCbdLogoLeaf" height="65" width="52" alt="Convention on Biological Diversity">
                        <img src="https://www.cbd.int/styles/ui/templates/cbd2011/images/logo-cbd-text-en.gif" id="CCbdLogoText" height="65" alt="Convention on Biological Diversity">
                    </a>
                </div>
        <!-- <div style="padding:5px 0px">
          <a href="/">
            <img class="logo" src="/app/images/cbd-logo-green-en.svg" />
          </a>
        </div> -->

        <div class="pull-right">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/convention">The Convention</a></li>
            <li class="active"><a href="/biosafety">Cartagena Protocol</a></li>
            <li class="active"><a href="/abs">Nagoya Protocol</a></li>
            <li class="active"><a href="/programmes">Programmes</a></li>
            <li class="active"><a href="/information">Information</a></li>
            <li class="active hidden-sm"><a href="/secretariat">Secretariat</a></li>
          </ul>
        </div>
        <div class="clearfix"></div>
      </div>
    </div>
  </header>

  <!-- CONTENT -->
  <!-- CONTENT -->
  <div class="container whitebg view">

    <portal-nav></portal-nav>
    <ng-view autoscroll="true"></ng-view>
  </div>

